export JUNIT=../junit/junit4.7/junit-4.7
mvn install:install-file -DgroupId=junit -DartifactId=junit -Dversion=4.7-CONCURRENT -DgeneratePom=true -Dpackaging=jar -Dfile=$JUNIT.jar
mvn install:install-file -DgroupId=junit -DartifactId=junit -Dversion=4.7-CONCURRENT -DgeneratePom=true -Dpackaging=jar -Dclassifier=sources -Dfile=$JUNIT-src.jar
