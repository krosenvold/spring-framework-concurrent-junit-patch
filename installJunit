mvn install:install-file -DgroupId=junit -DartifactId=junit -Dversion=4.7-CONCURRENT -DgeneratePom=true -Dpackaging=jar -Dfile=../junit/junit4.7/junit-4.7.jar
mvn install:install-file -DgroupId=junit -DartifactId=junit -Dversion=4.7-CONCURRENT -DgeneratePom=true -Dpackaging=jar -Dclassifier=sources -Dfile=../junit/junit4.7/junit-4.7-src.jar
