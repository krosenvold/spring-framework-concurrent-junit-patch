export ARTVERSION=1447
echo "Setting up version 3.0.0.BUILD$ARTVERSION"
mvn -q install:install-file -DgroupId=org.springframework -DartifactId=spring-test -Dversion=3.0.0.BUILD$ARTVERSION -DgeneratePom=true -Dpackaging=jar -Dfile=trunk/org.springframework.test/target/artifacts/org.springframework.test.jar
mvn -q install:install-file -DgroupId=org.springframework -DartifactId=spring-test -Dversion=3.0.0.BUILD$ARTVERSION -DgeneratePom=true -Dpackaging=jar -Dclassifier=sources -Dfile=trunk/org.springframework.test/target/artifacts/org.springframework.test-sources.jar
mvn -q install:install-file -DgroupId=org.springframework -DartifactId=spring-beans -Dversion=3.0.0.BUILD$ARTVERSION -DgeneratePom=true -Dpackaging=jar -Dfile=trunk/org.springframework.beans/target/artifacts/org.springframework.beans.jar
mvn -q install:install-file -DgroupId=org.springframework -DartifactId=spring-beans -Dversion=3.0.0.BUILD$ARTVERSION -DgeneratePom=true -Dpackaging=jar -Dclassifier=sources -Dfile=trunk/org.springframework.beans/target/artifacts/org.springframework.beans-sources.jar
mvn -q install:install-file -DgroupId=org.springframework -DartifactId=spring-context -Dversion=3.0.0.BUILD$ARTVERSION -DgeneratePom=true -Dpackaging=jar -Dfile=trunk/org.springframework.context/target/artifacts/org.springframework.context.jar
mvn -q install:install-file -DgroupId=org.springframework -DartifactId=spring-context -Dversion=3.0.0.BUILD$ARTVERSION -DgeneratePom=true -Dpackaging=jar -Dclassifier=sources -Dfile=trunk/org.springframework.context/target/artifacts/org.springframework.context-sources.jar
mvn -q install:install-file -DgroupId=org.springframework -DartifactId=spring-core -Dversion=3.0.0.BUILD$ARTVERSION -DgeneratePom=true -Dpackaging=jar -Dfile=trunk/org.springframework.core/target/artifacts/org.springframework.core.jar
