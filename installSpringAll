mvn -q install:install-file -DgroupId=org.springframework -DartifactId=spring-web -Dversion=3.0.0.BUILD$ARTVERSION -DgeneratePom=true -Dpackaging=jar -Dfile=trunk/org.springframework.web/target/artifacts/org.springframework.web.jar
